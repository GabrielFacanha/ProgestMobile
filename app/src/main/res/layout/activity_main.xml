<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <View
        android:id="@+id/divisor1"
        android:layout_width="1278dp"
        android:layout_height="3dp"
        android:background="?android:attr/listDivider"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="105dp" />

    <TextView
        android:id="@+id/logoProgest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PROGEST"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="29dp" />

    <View
        android:id="@+id/divisor2"
        android:layout_width="1278dp"
        android:layout_height="3dp"
        android:background="?android:attr/listDivider"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="419dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="1278dp"
        android:layout_height="311dp"
        android:background="#D2E1E9"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="108dp" />

    <ImageView
        android:id="@+id/miniaturaRelatoriofixo"
        android:layout_width="159dp"
        android:layout_height="211dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/cap_relatorio1__1_"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="167dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Iniciar novo documento"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="185dp"
        tools:layout_editor_absoluteY="117dp" />

    <SearchView
        android:id="@+id/barradepesquisa"
        android:layout_width="776dp"
        android:layout_height="49dp"
        android:hint="Pesquise por nome, assunto, turma..."
        android:queryHint="Pesquise por nome, assunto, turma..."
        android:visibility="visible"
        tools:layout_editor_absoluteX="374dp"
        tools:layout_editor_absoluteY="29dp" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="159dp"
        android:layout_height="211dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/imagem_parecer"
        tools:layout_editor_absoluteX="439dp"
        tools:layout_editor_absoluteY="167dp" />

    <ImageView
        android:id="@+id/fichaatendimento"
        android:layout_width="159dp"
        android:layout_height="211dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/ficha_de_atendimento"
        tools:layout_editor_absoluteX="642dp"
        tools:layout_editor_absoluteY="167dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Relatorio"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="380dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parecer"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="439dp"
        tools:layout_editor_absoluteY="380dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ficha"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="642dp"
        tools:layout_editor_absoluteY="380dp" />
</androidx.constraintlayout.widget.ConstraintLayout>